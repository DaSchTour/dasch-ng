{
  "name": "rxjs-operators",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/rxjs/operators/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/rxjs/operators"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/rxjs/operators/jest.config.ts"
      }
    },
    "publish": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public",
        "distFolderPath": "dist/libs/rxjs/operators"
      },
      "dependsOn": ["build"]
    },
    "github": {
      "executor": "@jscutlery/semver:github"
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "postTargets": ["rxjs-operators:publish", "rxjs-operators:github"]
      }
    }
  },
  "tags": []
}
