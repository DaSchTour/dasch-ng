{
  "name": "ng-utils",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/ng/utils/src",
  "prefix": "dasch-ng",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/ng/utils/ng-package.json",
        "tsConfig": "libs/ng/utils/tsconfig.lib.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/ng/utils/tsconfig.lib.prod.json"
        },
        "development": {}
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/ng/utils/jest.config.ts",
        "tsConfig": "libs/ng/utils/tsconfig.spec.json"
      },
      "configurations": {
        "update": {
          "updateSnapshot": true
        }
      }
    },
    "lint": {
      "inputs": [
        "default",
        "{workspaceRoot}/.eslintrc.json",
        "{workspaceRoot}/eslint.config.mjs",
        {
          "externalDependencies": ["eslint"]
        }
      ]
    }
  }
}
